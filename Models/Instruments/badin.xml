<?xml version="1.0"?>

<PropertyList>

 <name>badin</name>
 <path>badin.ac</path>

 <animation>
  <type>material</type>
  <object-name>cadran</object-name>
  <object-name>aiguille</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.1</blue>
   <factor-prop>/sim/model/material/instruments/factor</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>aiguille</object-name>
  <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
   <entry><ind>    0</ind><dep>    0</dep></entry>
   <entry><ind>   10</ind><dep>    3</dep></entry>
   <entry><ind>   18</ind><dep>   13</dep></entry>
   <entry><ind>   27</ind><dep>   27</dep></entry>
   <entry><ind>   54</ind><dep>   82</dep></entry>
   <entry><ind>  108</ind><dep>  192</dep></entry>
   <entry><ind>  162</ind><dep>  302</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


