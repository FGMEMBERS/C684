<?xml version="1.0"?>

<PropertyList>

<path>switch3.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>switch</object-name>
    <property>/controls/switches/selected_pitch</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  30 </dep></entry>
      <entry><ind> 1 </ind><dep>   0 </dep></entry>
      <entry><ind> 2 </ind><dep> -30 </dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.0</z-m>  
    </center>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>rectangle1</object-name>
    <visible>false</visible>
    <action>
      <repeatable>false</repeatable>
      <button>0</button>
      <binding>
        <command>property-assign</command>
        <property>/controls/switches/selected_pitch</property>
        <value>2</value>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/switch</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>rectangle2</object-name>
    <visible>false</visible>
    <action>
      <repeatable>false</repeatable>
      <button>0</button>
      <binding>
        <command>property-assign</command>
        <property>/controls/switches/selected_pitch</property>
        <value>1</value>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/switch</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>rectangle3</object-name>
    <visible>false</visible>
    <action>
      <repeatable>false</repeatable>
      <button>0</button>
      <binding>
        <command>property-assign</command>
        <property>/controls/switches/selected_pitch</property>
        <value>0</value>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/switch</property>
      </binding>
    </action>
  </animation>

</PropertyList>
