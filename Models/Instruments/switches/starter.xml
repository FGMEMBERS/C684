<?xml version="1.0"?>

<PropertyList>

<path>starter.ac</path>

  <animation>
    <type>translate</type>
    <object-name>bouton</object-name>
    <property>controls/switches/starter</property>
    <factor>-0.0022</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.0</z-m>  
    </center>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>bouton</object-name>
    <object-name>base</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <binding>
        <command>nasal</command>
        <script>C684.starter(1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>C684.starter(0)</script>
        </binding>
      </mod-up>
    </action>
  </animation>

</PropertyList>
