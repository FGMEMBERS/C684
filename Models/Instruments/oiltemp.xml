<?xml version="1.0"?>

<PropertyList>

  <path>oiltemp.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>aiguille</object-name>
    <property>engines/engine/oil-temperature-degf</property>
    <interpolation>
      <entry>
        <ind>32</ind>
        <dep>-50</dep>
      </entry>
      <entry>
        <ind>104</ind>
        <dep>-30</dep>
      </entry>
      <entry>
        <ind>140</ind>
        <dep>-14.3</dep>
      </entry>
      <entry>
        <ind>176</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>212</ind>
        <dep>13.0</dep>
      </entry>
      <entry>
        <ind>266</ind>
        <dep>30</dep>
      </entry>
      <entry>
        <ind>350</ind>
        <dep>35</dep>
      </entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m> 0</x-m>
      <y-m> 0</y-m>
      <z-m>-0.008</z-m>
    </center>
  </animation>

</PropertyList>
