<?xml version="1.0"?>
<PropertyList>

<path>flaps.ac</path>

<animation>
  <type>translate</type>
  <object-name>selecteur</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry>
      <ind>0</ind>
      <dep>0.0348</dep>
    </entry>
    <entry>
      <ind>0.375</ind>
      <dep>0.0088</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>-0.0351</dep>
    </entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>1</z>
  </axis>
</animation>

<animation>
  <type>pick</type>
  <object-name>selecteur</object-name>
  <action>
    <button>0</button>
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(1)</script>
    </binding>
  </action>
  <action>
    <button>1</button>
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(-1)</script>
    </binding>
  </action>
</animation>

</PropertyList>
