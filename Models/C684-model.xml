<?xml version="1.0"?>

<PropertyList>

 <path>C684.ac</path>
  <offsets>
   <x-m> -3.00</x-m>
   <y-m>  0.00</y-m>
   <z-m>  0.00</z-m>
  </offsets>

<!-- groupes -->
 <animation>
  <name>helice</name>
  <object-name>cone</object-name>
  <object-name>pale1</object-name>
  <object-name>pale2</object-name>
 </animation>


<!-- helice -->
 <animation>
  <type>shader</type>
  <shader>chrome</shader>
  <texture>chrome.png</texture>
  <object-name>pale1</object-name>
  <object-name>pale2</object-name>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>helice</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>1</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<!-- ailerons -->
 <animation>
  <type>rotate</type>
  <object-name>aileronG</object-name>
  <property>surface-positions/left-aileron-pos-norm</property>
  <factor>-15</factor>
  <axis>
   <x1-m>2.73</x1-m>
   <y1-m>-2.27</y1-m>
   <z1-m>-0.37</z1-m>
   <x2-m>2.62</x2-m>
   <y2-m>-3.42</y2-m>
   <z2-m>-0.33</z2-m>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>aileronD</object-name>
  <property>surface-positions/right-aileron-pos-norm</property>
  <factor>-15</factor>
  <axis>
   <x1-m>2.73</x1-m>
   <y1-m>2.27</y1-m>
   <z1-m>-0.37</z1-m>
   <x2-m>2.62</x2-m>
   <y2-m>3.42</y2-m>
   <z2-m>-0.33</z2-m>
  </axis>
 </animation>

<!-- profondeur -->
 <animation>
  <type>rotate</type>
  <object-name>gouvProfG</object-name>
  <object-name>gouvProfD</object-name>
  <property>surface-positions/elevator-pos-norm</property>
  <factor>20</factor>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
  <center>
   <x-m>6.30</x-m>
   <y-m>0.00</y-m>
   <z-m>0.07</z-m>
  </center>
 </animation>

<!-- direction -->
 <animation>
  <type>rotate</type>
  <object-name>gouvDir</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>20</factor>
  <axis>
   <x1-m>6.51</x1-m>
   <y1-m>0.00</y1-m>
   <z1-m>0.14</z1-m>
   <x2-m>6.52</x2-m>
   <y2-m>0.00</y2-m>
   <z2-m>1.01</z2-m>
  </axis>
 </animation>

<!-- volets -->
 <animation>
  <type>rotate</type>
  <object-name>voletG</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <factor>-40</factor>
  <axis>
   <x1-m>2.93</x1-m>
   <y1-m>-0.39</y1-m>
   <z1-m>-0.45</z1-m>
   <x2-m>2.74</x2-m>
   <y2-m>-2.27</y2-m>
   <z2-m>-0.37</z2-m>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>voletD</object-name>
  <property>surface-positions/flap-pos-norm</property>
  <factor>40</factor>
  <axis>
   <x1-m>2.93</x1-m>
   <y1-m>0.39</y1-m>
   <z1-m>-0.45</z1-m>
   <x2-m>2.74</x2-m>
   <y2-m>2.27</y2-m>
   <z2-m>-0.37</z2-m>
  </axis>
 </animation>

<!-- enfoncement du train -->
 <animation>
  <type>translate</type>
  <object-name>roueG</object-name>
  <object-name>carenageG</object-name>
  <property>gear/gear[0]/compression-norm</property>
  <factor>0.25</factor>
  <axis>
   <x>0.15</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>roueD</object-name>
  <object-name>carenageD</object-name>
  <property>gear/gear[1]/compression-norm</property>
  <factor>0.25</factor>
  <axis>
   <x>0.15</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

<!-- rotation des roues -->
 <animation>
  <type>spin</type>
  <object-name>roueG</object-name>
  <object-name>roueD</object-name>
  <property>velocities/groundspeed-kt</property>
  <factor>-10</factor>
  <center>
   <x-m>2.111</x-m>
   <y-m>0</y-m>
   <z-m>-1.112</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<!-- verriere -->
  <animation>
    <type>select</type>
    <object-name>verriere</object-name>
    <condition>
      <not-equals>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </not-equals>
    </condition>
  </animation>


</PropertyList>
