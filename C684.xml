        <z>  -0.25 </z>
      </location>
      <orient unit="DEG">
        <pitch> 0.00 </pitch>
        <roll>  0.00 </roll>
        <yaw>   0.00 </yaw>
      </orient>
      <feed>0</feed>
      <feed>1</feed>
      <thruster file="ratier_variable">
        <location unit="M">
          <x>   0.33 </x>
          <y>   0.00 </y>
          <z>   0.00 </z>
        </location>
        <orient unit="DEG">
          <pitch> 0.00 </pitch>
          <roll>  0.00 </roll>
          <yaw>   0.00 </yaw>
        </orient>
        <sense>-1</sense>
        <p_factor>10</p_factor>
